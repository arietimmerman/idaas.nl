{
  "name": "idaas.nl/idaas.nl",
  "description": "idaas.nl is (not) yet another identity platform ",
  "keywords": ["framework", "laravel"],
  "license": "Commons Clause",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "arietimmerman/laravel-saml": "^0",
    "arietimmerman/laravel-scim-server": "^0",
    "aws/aws-sdk-php": "^3.81",
    "doctrine/dbal": "^2.9",
    "guzzlehttp/guzzle": "^7.0.1",
    "innocenzi/laravel-vite": "0.2.*",
    "laravel/framework": "^9",
    "laravel/legacy-factories": "^1.3",
    "laravel/socialite": "^5.3",
    "laravel/tinker": "^2.0",
    "lbuchs/webauthn": "^1.1",
    "lcobucci/jwt": "^4.2",
    "mustache/mustache": "^2.12",
    "nl.idaas/laravel-openid-connect": "^0",
    "phpunit/phpunit": "^9.0",
    "predis/predis": "^1.1",
    "sdboyer/gliph": "^0.7.0",
    "spomky-labs/otphp": "^10",
    "tijsverkoyen/css-to-inline-styles": "^2.2",
    "vemcogroup/laravel-sparkpost-driver": "^5"
  },
  "require-dev": {
    "composer/composer": "^2",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "laravel/sail": "^1.15",
    "mockery/mockery": "~1.0",
    "symfony/thanks": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "symfony/thanks": false
    }
  }
}
